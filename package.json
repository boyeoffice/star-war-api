{
  "name": "starwar-api",
  "version": "1.0.0",
  "description": "Star war node app",
  "main": "index.js",
  "engines": {
    "node": "12.20.0"
  },
  "scripts": {
    "dev": "nodemon ./src/index.js",
    "test:dev": "NODE_ENV=test npm run test",
    "start": "node index.js",
    "seed": "node ./src/database/seeders/admin.seeder adminSeed",
    "migrate": "node ./src/database createAllTables",
    "migrate:rollback": "node ./src/database dropAllTables",
    "test": "mocha --recursive --exit --timeout 120000",
    "coverage": "nyc npm test && nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/boyeoffice/Teamwork-backend.git"
  },
  "author": "Olakunle Boye",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/boyeoffice/Teamwork-backend/issues"
  },
  "homepage": "https://github.com/boyeoffice/Teamwork-backend#readme",
  "dependencies": {
    "bcrypt": "^5.0.0",
    "cloudinary": "^1.23.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "http-errors": "^1.8.0",
    "jsonwebtoken": "^8.5.1",
    "make-runnable": "^1.3.6",
    "nodemailer": "^6.4.16",
    "pg": "^8.3.0",
    "swapi-node": "^0.6.0",
    "validatorjs": "^3.20.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "coveralls": "^3.1.0",
    "eslint": "^7.8.1",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "jest": "^26.4.2",
    "mocha": "^8.1.3",
    "nodemon": "^2.0.4",
    "nyc": "^15.1.0",
    "supertest": "^4.0.2"
  }
}
